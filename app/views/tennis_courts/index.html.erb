<h3>tennis courts</h3>

<% @tennis_courts.each do |tennis_court| %>
  <div class="tennis_court">
    <%= tennis_court.name %>
    <%= link_to "Show all bookings", tennis_court_bookings_path(tennis_court) %>
    <%= link_to "Delete", tennis_court_path(tennis_court), data: { confirm: 'Are you sure?' }, method: :delete %>
    <%= link_to "Edit", edit_tennis_court_path(tennis_court) %>
    <%= link_to "Add booking", new_tennis_court_booking_path(tennis_court) %>
  </div>
<% end %>
