<h3> Bookings for 
  <%= select_tag :court,
      options_for_select(TennisCourt.all.map {|c| [ c.name, tennis_court_bookings_path(c) ]}, tennis_court_bookings_path(@tennis_court))
  %>
</h3>

<div class="calendar_container">
  <div id="calendar">
  </div>
</div>
<table>
  <tr>
    <th>Start</th>
    <th>End</th>
  </tr>
  
  <% @bookings.each do |booking| %>
  <tr>
    <td>
      <%= booking.start_time.strftime('%e %b %Y %H:%M%p') %>
    </td>
    <td>
      <%= (booking.calculate_end_time + 60).strftime('%e %b %Y %H:%M%p') %>
    </td>
    <td><%= link_to "Delete", tennis_court_booking_path(@tennis_court, booking), data: { confirm: 'Are you sure?' }, method: :delete %>
    </td>
    <td><%= link_to "Edit", edit_tennis_court_booking_path(@tennis_court, booking) %>
    </td>
  </tr>
  <% end %>
</table>